#문제 3

a = {'A':90, 'B':80, 'C':70}
#%%
#첫 단계
a.get('B')
del a['B']
#%%
#또는
a = {'A':90, 'B':80, 'C':70}
r = a.pop('B')
print(r)
#%%
#문제 4
a.get('D', 70)
#%%
